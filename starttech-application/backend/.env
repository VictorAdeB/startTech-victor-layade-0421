# Server
PORT=8080

# MongoDB Atlas (external, from Terraform / GitHub Secrets)
MONGO_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/starttech
DB_NAME=starttechdb

# JWT
JWT_SECRET_KEY=your_super_secret_key
JWT_EXPIRATION_HOURS=72

# Cache
ENABLE_CACHE=true
REDIS_ADDR=<redis-endpoint>:6379
REDIS_PASSWORD=          # leave empty if no password on Redis

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Cookies & Security
COOKIE_DOMAINS=localhost
SECURE_COOKIE=false
ALLOWED_ORIGINS=http://localhost:5173

